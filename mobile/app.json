{
  "expo": {
    "name": "Wild West Brew Fest",
    "slug": "brew-fest-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.brewfestmobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.brewfestmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c682fd72-fa21-42ee-9a7a-6383356363bc"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins/Poppins-Regular.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/WWBF-KATYImage.png",
          "imageWidth": 200
        }
      ],
      "expo-router"
    ]
  }
}
